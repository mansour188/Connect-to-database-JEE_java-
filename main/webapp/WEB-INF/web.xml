<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
     <servlet-name>Test</servlet-name>
    <servlet-class>com.servelet.Test</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Test</servlet-name>
    <url-pattern>/bonjour</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name> Accueil</servlet-name>
  <servlet-class>com.servelet.Accueil</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Accueil</servlet-name>
  <url-pattern>/accueil</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Relation</servlet-name>
  <servlet-class>com.servelet.Relation</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Relation</servlet-name>
  <url-pattern>/info</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>Form</servlet-name>
  <servlet-class>com.servelet.Form</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Form</servlet-name>
  <url-pattern>/form</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>New</servlet-name>
  <servlet-class>com.servelet.New</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>New</servlet-name>
  <url-pattern>/new</url-pattern>
  </servlet-mapping>
  <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>
</web-app>